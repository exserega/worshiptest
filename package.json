{
  "name": "agape-worship",
  "private": true,
  "version": "0.0.0",
  "description": "Static dev server and lint config for local development",
  "dependencies": {
    "@capacitor/app": "^6.2.0",
    "@capacitor/browser": "^6.2.0",
    "@capacitor/splash-screen": "^6.2.0"
  },
  "scripts": {
    "start": "http-server -p 8001 -c-1 .",
    "lint": "eslint --ext .js ./src ./js ./public || exit 0",
    "cap:init": "npx cap init \"Agape Worship\" com.agapeworship.app --web-dir=.",
    "cap:sync": "npx cap sync",
    "cap:open:android": "npx cap open android",
    "cap:open:ios": "npx cap open ios"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "http-server": "^14.1.1",
    "@capacitor/cli": "^6.2.0",
    "@capacitor/core": "^6.2.0"
  }
}


